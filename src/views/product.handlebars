<h1>Products</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">NAME</th>
      <th scope="col">PRICE</th>
    </tr>
  </thead>
  <tbody>

    {{#each data}}

      <tr>
        <th scope="row">{{id}}</th>
        <td>{{name}}</td>
        <td>{{price}}</td>
      </tr>

    {{/each}}

  </tbody>
</table>